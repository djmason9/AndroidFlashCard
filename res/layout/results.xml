<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="@drawable/port_bg"
	android:padding="10sp">

	<TableLayout android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:id="@+id/resultsLayout" android:background="@drawable/prefbg">
		<TextView android:id="@+id/tvTitle" android:layout_height="wrap_content"
			android:background="@drawable/resultshdr" android:text="@string/resultshead"
			android:textColor="#fff" android:textSize="18sp" android:paddingTop="6sp"
			android:gravity="center_horizontal" android:layout_margin="10sp"
			android:layout_width="fill_parent"></TextView>
		<TableLayout android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/tbInfo">
		<TextView android:layout_height="wrap_content" android:text="@+id/TextView01"
			android:id="@+id/tvToBeStudied"
			android:layout_width="fill_parent" android:background="#dfdfdf"
			android:textColor="#000" android:paddingTop="2sp"
			android:paddingBottom="2sp" android:textSize="14sp"
			android:layout_marginLeft="10sp" android:layout_marginRight="10sp" android:paddingLeft="10sp"></TextView>
		<TextView android:layout_height="wrap_content" android:text="@+id/TextView01"
			android:id="@+id/tvSeen"
			android:layout_width="fill_parent" android:textColor="#000"
			android:paddingTop="2sp" android:paddingBottom="2sp"
			android:textSize="14sp" android:layout_marginLeft="10sp"
			android:layout_marginRight="10sp" android:paddingLeft="10sp"></TextView>
		<TextView android:layout_height="wrap_content" android:text="@+id/TextView01"
			android:id="@+id/tvIncorrect"
			android:layout_width="fill_parent" android:background="#dfdfdf"
			android:textColor="#000" android:paddingTop="2sp"
			android:paddingBottom="2sp" android:textSize="14sp"
			android:layout_marginLeft="10sp" android:layout_marginRight="10sp" android:paddingLeft="10sp"></TextView>
		<TextView android:layout_height="wrap_content" android:text="@+id/TextView01"
			android:id="@+id/tvCorrect"
			android:layout_width="fill_parent" android:textColor="#000"
			android:paddingTop="2sp" android:paddingBottom="2sp"
			android:textSize="14sp" android:layout_marginLeft="10sp"
			android:layout_marginRight="10sp" android:paddingLeft="10sp"></TextView>
</TableLayout>
		<LinearLayout android:layout_height="wrap_content"
			android:id="@+id/vResultsChart" android:layout_width="fill_parent"
			android:layout_marginTop="5sp">
			</LinearLayout> <Button android:layout_height="wrap_content"
			android:text="@string/butRetest"
			android:background="@drawable/button" android:paddingLeft="30sp" android:paddingTop="0sp"
			android:paddingRight="40sp" android:id="@+id/btRetest"
			android:layout_width="fill_parent" android:layout_marginTop="10sp"
			android:layout_margin="10sp" android:paddingBottom="8sp" android:textStyle="bold" android:textColor="#4c493f"> </Button
		>

	
</TableLayout>

</LinearLayout>
