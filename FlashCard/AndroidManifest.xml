<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.flashcard" android:versionName="1.0.9" android:versionCode="12">
	<application android:icon="@drawable/icon" android:label="@string/app_name"
		android:debuggable="false">
		<activity android:name=".view.Home" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

		<activity android:name=".view.InstructionsActivity" android:theme="@style/Theme.Translucent"></activity>
		<activity android:name=".view.HomeInstructionsActivity" android:theme="@style/Theme.Translucent"></activity>

		<activity android:name=".view.CardSetList"></activity>
		<activity android:name=".view.FlashCardTest"></activity>
		<activity android:name=".view.Results"></activity>
		<activity android:name=".view.UserPrefActivity"></activity>
		<activity android:name=".view.BookmarkList"></activity>
		<activity android:name=".view.OfflineListView"></activity>
		<activity android:name=".view.OfflineCardsList"></activity>
	</application>
	<uses-sdk android:minSdkVersion="3" />

	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest> 